{
  "name": "oauth-proxy",
  "version": "1.0.0",
  "description": "OAuth proxy service",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "build": "pkg . && node build.js",
    "dev": "nodemon src/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": "server.js",
    "assets": "public/**/*",
    "targets": [
      "node18-macos-arm64"
    ],
    "outputPath": "dist"
  }
}
